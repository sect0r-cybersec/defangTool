{
  "manifest_version": 3,
  "name": "IOC Defang Extension",
  "version": "0.5.4",
  "description": "Defang IOCs (IPs, URLs, domains, emails) following IETF draft standards",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon.svg",
    "128": "icon.svg"
  }
} 